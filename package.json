{
  "name": "cy-network-store",
  "author": "Keiichiro Ono",
  "version": "2.0.0",
  "description": "Redux data store for CyNetworkViewer",
  "license": "UCSD",
  "keywords": [
    "cytoscape",
    "visualization",
    "graph",
    "bioinformatics"
  ],
  "main": "build/CyNetworkStore.js",
  "files": [
    "webpack.config.js",
    ".preprocessor.js",
    ".travis.yml",
    "build/*",
    "src/*"
  ],
  "homepage": "https://github.com/CyComponent/CyNetworkStore",
  "repository": {
    "type": "git",
    "url": "https://github.com/CyComponent/CyNetworkStore.git"
  },
  "bugs": {
    "url": "https://github.com/CyComponent/CyNetworkStore/issues"
  },
  "scripts": {
    "build": "webpack --progress --colors",
    "clean": "rm build/*",
    "lint": "eslint src/; exit 0",
    "test": "eslint src/ && jest",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "immutable": "^3.8.1"
  },
  "devDependencies": {
    "babel-core": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "eslint": "^3.7.1",
    "jest-cli": "^16.0.1",
    "redux": "^3.6.0",
    "webpack": "^1.13.2"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/.preprocessor.js"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "eslint:recommended",
    "ecmaFeatures": {
      "modules": true
    },
    "env": {
      "es6": true,
      "browser": true
    }
  }
}
